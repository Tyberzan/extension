{
    "manifest_version": 3,
    "name": "Extension_Mail",
    "version": "1.0.0",
    "description": "Extension en cours de developpement",
    "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

    "permissions": [
        "activeTab",
        "scripting",
        "storage"
        
    ],
    "host_permissions": [
    "https://script.google.com/"
  ],
     "action":{
        "default_popup": "Extension_Mail.html"
    },
    "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://script.google.com"
  },
    "oauth2":{  
      "client_id":"https://script.googleapis.com/v1/scripts/AKfycbwMLfQI-buq5IqCpWQxv4CEXbBG2gJDcmvssHI8g8mNsi1AJ_AtniVU1vPl7yqFRElNxA",
        "scopes":[  
           "https://www.googleapis.com/auth/drive"
        ]
     },
     "key":""
}